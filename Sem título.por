programa {
  funcao inicio() {
    real num1, num2, soma, resultado
    escreva ("Informe o primeiro número da soma e subtração: ")
    leia (num1)
    escreva ("Informe o segundo número para somar e subtrair: ")
    leia (num2)
    soma = num1 + num2
    escreva ("O resultado é:" + soma "\n")
    resultado = num1 - num2
    escreva ("O resultado final é: " + resultado)
  }
}
